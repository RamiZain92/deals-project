extend type Query {
	getAllReviews(pageNum : Int,pageSize : Int,keyword : String) : [DriverReviewResponse]
	getAllReviewsOfDriver(id : Long) : [DriverReviewResponse]
	getAvgRatingOfDriver(id : Long) : Float
}
type DriverReviewResponse{
	id :  Long 
	customerId : Long
	driverFirstName : String 
	driverLastName : String
	rating : Int
	feedback : String
	suggestion : String 
}